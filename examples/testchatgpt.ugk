importer <math.ugk>

fonction entier factorial(entier n) debut
    entier result := 1
    pour i allant de 1 vers n faire
        result := result * i
    fpour
    retourne result
fin

fonction double fibonacci(entier n) debut
    double result
    si n <= 1 alors
        result := n
    sinon
        result := fibonacci(n - 1) + fibonacci(n - 2)
    fsi
    retourne result
fin

fonction entier test(entier a) debut
    selon a faire
        cas 1 faire retourne 6
        cas 2 faire retourne 1
        defaut faire retourne 0
    fselon
fin

algorithme debut
    entier i
    pour i allant de 1 vers 3999 faire
        afficher(to_roman(i))
    fpour
fin